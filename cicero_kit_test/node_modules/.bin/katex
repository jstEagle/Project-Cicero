#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/Github_Repo_Justus/Project-Cicero/cicero_kit_test/node_modules/katex/node_modules:/mnt/d/Github_Repo_Justus/Project-Cicero/cicero_kit_test/node_modules:/mnt/d/Github_Repo_Justus/Project-Cicero/node_modules:/mnt/d/Github_Repo_Justus/node_modules:/mnt/d/node_modules:/mnt/d/Github_Repo_Justus/Project-Cicero/cicero_kit_test/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/Github_Repo_Justus/Project-Cicero/cicero_kit_test/node_modules/katex/node_modules:/mnt/d/Github_Repo_Justus/Project-Cicero/cicero_kit_test/node_modules:/mnt/d/Github_Repo_Justus/Project-Cicero/node_modules:/mnt/d/Github_Repo_Justus/node_modules:/mnt/d/node_modules:/mnt/d/Github_Repo_Justus/Project-Cicero/cicero_kit_test/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../katex/cli.js" "$@"
else
  exec node  "$basedir/../katex/cli.js" "$@"
fi
